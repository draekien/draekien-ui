{
  "name": "draekien-ui",
  "version": "0.0.0-semantic-released",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/draekien/draekien-ui"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "commit": "git-cz",
    "start": "start-storybook -p 6006 -s public --no-dll",
    "start-storybook": "start-storybook -p 6006 -s ./.storybook/static --no-dll",
    "test": "jest --coverage",
    "test-update": "jest --updateSnapshot",
    "test-watch": "jest --watch",
    "docs": "jest-coverage-badges --output \"./badges\"",
    "build": "npm run lint && npm run test && tsc --noEmit false",
    "build-storybook": "build-storybook -c .storybook -o storybook-static --no-dll",
    "build-all": "concurrently \"npm run build\" \" npm run build-storybook\"",
    "release": "semantic-release --no-ci",
    "release-chromatic": "npx chromatic",
    "lint": "eslint \"*.js\" \"**/*.tsx\" \"**/*.ts\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1",
    "react-router-dom": ">=5.2.0",
    "theme-ui": ">=0.6.0-alpha.7"
  },
  "dependencies": {
    "@emotion/react": "11.1.4",
    "@theme-ui/color": "0.6.0-alpha.6",
    "jest-coverage-badges": "1.1.2",
    "react-autosize-textarea": "^7.1.0",
    "react-custom-scrollbars": "4.2.1",
    "react-input-mask": "2.0.4",
    "react-outside-click-handler": "^1.3.0",
    "react-switch": "^5.0.1",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.7",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@storybook/addon-actions": "6.2.7",
    "@storybook/addon-essentials": "6.2.7",
    "@storybook/addon-knobs": "6.2.7",
    "@storybook/addon-links": "6.2.7",
    "@storybook/node-logger": "6.2.7",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.2.7",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "^26.0.15",
    "@types/node": "12.20.5",
    "@types/react": "16.14.5",
    "@types/react-custom-scrollbars": "4.0.7",
    "@types/react-dom": "16.9.11",
    "@types/react-input-mask": "2.0.5",
    "@types/react-outside-click-handler": "^1.3.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-transition-group": "4.4.1",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "babel-loader": "8.2.1",
    "canvas": "2.6.1",
    "chromatic": "5.6.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "lint-staged": "10.5.1",
    "prettier": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "semantic-release": "17.4.2",
    "theme-ui": "0.6.0-alpha.7",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
